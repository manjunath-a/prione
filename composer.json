{
    "name": "Compassitesinc",
    "description": "Prione Applciation",
    "keywords": ["laravel", "Compassites"],
    "license": "MIT",
    "authors": [
        {
            "name": "Compassitesinc",
            "homepage": "http://www.Compassitesinc.com"
        }
    ],
    "require": {
        "laravel/framework": "~4.2",
        "zizaco/confide": "~4.0",
        "zizaco/entrust": "1.2.*",
        "bllim/datatables": "~1.3",
        "auth0/auth0-php": "0.6.*",
        "adoy/oauth2": "dev-master",
        "mgallegos/laravel-jqgrid": "1.*",
        "aws/aws-sdk-php-laravel": "1.*",
        "doctrine/dbal":"dev-master",
        "barryvdh/laravel-ide-helper": "~1.11"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "~1.8",
        "way/generators": "~2.6",
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "summerstreet/woodling": "~0.1.6"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/library",
            "app/models",
            "app/services",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/library"
        ],
        "psr-0": {
            "Acme": "app/"
        }
    },
    "scripts": {
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-install-cmd": [
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
